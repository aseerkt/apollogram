{
   "name": "server",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "main": "dist/index.js",
   "scripts": {
      "start": "node dist/index.js",
      "watch": "npx tsc -w",
      "server": "nodemon --ignore client/ dist/index.js",
      "dev": "concurrently \"yarn watch\" \"yarn server\" -c blue.bold,yellow.bold -n TSC,SERVER",
      "typeorm": "ts-node ./node_modules/typeorm/cli.js",
      "seed": "npx tsc && ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
      "heroku-postbuild": "npx tsc"
   },
   "dependencies": {
      "@types/cookie-parser": "^1.4.2",
      "@types/express": "^4.17.11",
      "@types/express-rate-limit": "^5.1.1",
      "@types/graphql-upload": "^8.0.4",
      "@types/jsonwebtoken": "^8.5.5",
      "@types/node": "^16.7.1",
      "@types/sharp": "^0.28.5",
      "apollo-server-express": "^3.1.2",
      "argon2": "^0.28.2",
      "class-validator": "^0.13.1",
      "cloudinary": "^1.26.3",
      "colors": "^1.4.0",
      "cookie-parser": "^1.4.5",
      "dataloader": "^2.0.0",
      "dotenv": "^10.0.0",
      "express": "^4.17.1",
      "express-rate-limit": "^5.2.6",
      "graphql": "^15.5.0",
      "graphql-upload": "^12.0.0",
      "jsonwebtoken": "^8.5.1",
      "npm-force-resolutions": "^0.0.10",
      "pg": "^8.7.1",
      "reflect-metadata": "^0.1.10",
      "sharp": "^0.29.0",
      "ts-node": "10.2.1",
      "type-graphql": "^1.1.1",
      "typeorm": "0.2.37",
      "typeorm-seeding": "^1.6.1",
      "typescript": "^4.3.5"
   },
   "devDependencies": {
      "concurrently": "^6.0.0",
      "nodemon": "^2.0.7"
   },
   "license": "MIT"
}
