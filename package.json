{
   "name": "server",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "main": "dist/index.js",
   "scripts": {
      "start": "node dist/index.js",
      "watch": "npx tsc -w",
      "server": "nodemon --ignore client/ dist/index.js",
      "dev": "concurrently \"yarn watch\" \"yarn server\" -n tsc, server",
      "typeorm": "ts-node ./node_modules/typeorm/cli.js",
      "seed": "npx tsc && ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
      "heroku-postbuild": "npx tsc && cd client && yarn && yarn build"
   },
   "dependencies": {
      "apollo-server-express": "^2.21.1",
      "argon2": "^0.27.1",
      "class-validator": "^0.13.1",
      "colors": "^1.4.0",
      "cookie-parser": "^1.4.5",
      "dataloader": "^2.0.0",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "graphql": "^15.5.0",
      "graphql-upload": "^11.0.0",
      "imagemin": "^7.0.1",
      "imagemin-mozjpeg": "^9.0.0",
      "is-jpg": "^2.0.0",
      "jsonwebtoken": "^8.5.1",
      "pg": "^8.5.1",
      "reflect-metadata": "^0.1.10",
      "sharp": "^0.27.2",
      "type-graphql": "^1.1.1",
      "typeorm": "0.2.31",
      "typeorm-seeding": "^1.6.1"
   },
   "devDependencies": {
      "@types/cookie-parser": "^1.4.2",
      "@types/express": "^4.17.11",
      "@types/graphql-upload": "^8.0.4",
      "@types/jsonwebtoken": "^8.5.1",
      "@types/node": "^14.14.35",
      "concurrently": "^6.0.0",
      "nodemon": "^2.0.7",
      "npm-force-resolutions": "^0.0.10",
      "ts-node": "9.1.1",
      "typescript": "4.2.3"
   },
   "license": "MIT",
   "resolutions": {
      "fs-capacitor": "^6.2.0",
      "graphql-upload": "^11.0.0"
   }
}
